-- Scripts:

local function MOBC_fake_script() -- Open.LocalScript 
	local script = Instance.new('LocalScript', Open)

	local open = script.Parent
	local frame = script.Parent.Parent.Frame
	
	open.MouseButton1Down:Connect(function()
		if frame.Position == UDim2.new(0.127,0,0.999,1) then
			frame:TweenPosition(UDim2.new(0.127,0,0.359,0),"Out","Back",1)
			open.Text = "Close"
	
		elseif frame.Position == UDim2.new(0.127,0,0.359,0) then
	
			frame:TweenPosition(UDim2.new(0.127,0,0.999,1),"Out","Back",1)
			open.Text = "Open"
		end
	end)
	
end
coroutine.wrap(MOBC_fake_script)()
local function VZOWFYZ_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local equis = script.Parent
	local frame = script.Parent.Parent
	local boton = script.Parent.Parent.Parent.Open
	
	equis.MouseButton1Down:Connect(function()
		frame:TweenPosition(UDim2.new(0.127,0,0.999,1),"In","Elastic",1)
		boton.Text = "Open"
	end)
end
coroutine.wrap(VZOWFYZ_fake_script)()
local function ZTWLYEL_fake_script() -- UniversalTab.LocalScript 
	local script = Instance.new('LocalScript', UniversalTab)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.CreditsTabFrame.Visible = false
		script.Parent.Parent.UniversalTabFrame.Visible = true
		script.Parent.Parent.FeTabFrame.Visible = false
		script.Parent.Parent.BdTabFrame.Visible = false
		
	
	end)
end
coroutine.wrap(ZTWLYEL_fake_script)()
local function YTUL_fake_script() -- CreditsTab.LocalScript 
	local script = Instance.new('LocalScript', CreditsTab)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.CreditsTabFrame.Visible = true
		script.Parent.Parent.UniversalTabFrame.Visible = false
		script.Parent.Parent.FeTabFrame.Visible = false
		script.Parent.Parent.BdTabFrame.Visible = false
	end)
end
coroutine.wrap(YTUL_fake_script)()
local function DGJA_fake_script() -- FeTab.LocalScript 
	local script = Instance.new('LocalScript', FeTab)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.CreditsTabFrame.Visible = false
		script.Parent.Parent.UniversalTabFrame.Visible = false
		script.Parent.Parent.FeTabFrame.Visible = true
		script.Parent.Parent.BdTabFrame.Visible = false
	
	end)
end
coroutine.wrap(DGJA_fake_script)()
local function FIRI_fake_script() -- BdTab.LocalScript 
	local script = Instance.new('LocalScript', BdTab)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.CreditsTabFrame.Visible = false
		script.Parent.Parent.UniversalTabFrame.Visible = false
		script.Parent.Parent.FeTabFrame.Visible = false
		script.Parent.Parent.BdTabFrame.Visible = true
	
	end)
end
